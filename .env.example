TRAEFIK_ACME_EMAIL=admin@example.com
TRAEFIK_APP_HOST=app.example.com
TRAEFIK_CERT_MAIN_DOMAIN=example.com
TRAEFIK_CERT_SANS=*.example.com
TRAEFIK_DNS_PROVIDER=cloudflare
TRAEFIK_DNS_RESOLVERS=1.1.1.1:53,8.8.8.8:53
DOCKER_API_VERSION=1.44
# CI-built production app image (GitHub Container Registry example).
APP_IMAGE=ghcr.io/xsybau/x-sub-store:latest
# Production by default; use staging during initial tests to avoid rate limits.
# LETSENCRYPT_CA_SERVER=https://acme-staging-v02.api.letsencrypt.org/directory
# Cloudflare token with DNS edit access for your zone.
CF_DNS_API_TOKEN=

APP_SECRET=change_this_to_32_byte_string
MONGO_URL=mongodb://mongo:27017/v2ray-hub
ADMIN_SESSION_SECRET=change_this_too_or_reuse_app_secret
ADMIN_AUTH_TOKEN_EXPIRES_SECONDS=86400
ADMIN_REFRESH_TOKEN_EXPIRES_SECONDS=604800
# PEM keys can use "\n" for line breaks in env values.
ADMIN_AUTH_PRIVATE_KEY=
ADMIN_AUTH_PUBLIC_KEY=
ADMIN_REFRESH_PRIVATE_KEY=
ADMIN_REFRESH_PUBLIC_KEY=
FETCH_TIMEOUT_MS=30000
DEV_DOMAIN=localhost
NUXT_PUBLIC_SUBSCRIPTION_BASE_URL=
